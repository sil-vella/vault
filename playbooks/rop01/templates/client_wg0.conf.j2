[Interface]
PrivateKey = {{ lookup('file', '/etc/wireguard/client_private.key') | trim }}
Address = 10.0.0.2/24
MTU = {{ wireguard_mtu }}
SaveConfig = true
DNS = 1.1.1.1

[Peer]
PublicKey = {{ wireguard_server_public_key }}
Endpoint = {{ wireguard_server_endpoint }}
AllowedIPs = {{ wireguard_allowed_ips }} 